@article{langton2021,
  title = {Cartograms, Hexograms and Regular Grids: {{Minimising}} Misrepresentation in Spatial Data Visualisations},
  shorttitle = {Cartograms, Hexograms and Regular Grids},
  author = {Langton, Samuel H and Solymosi, Reka},
  year = {2021},
